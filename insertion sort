#include <stdio.h>
void insertionsort(int a[],int n)
{
    int i,j,key;
    for(i=1;i<n;i++)
    {
        j=i-1;
        key=a[i];
        while(j>=0 && a[j]>key)
        {
            a[j+1]=a[j];
            j=j-1;
        }
        a[j+1]=key;
    }
}

int main()
{
    int a[20],i,n;
    printf("enter no of elements\n");
    scanf("%d",&n);
    printf("enter elements of array\n");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("\nelements before sorting\n");
    for(i=0;i<n;i++)
    printf("%d ",a[i]);
    insertionsort(a,n);
    printf("\nelements after sorting are\n");
    for(i=0;i<n;i++)
    printf("%d ",a[i]);

    return 0;
}
